{
    "name": "reactive-http",
    "version": "0.0.4",
    "license": "MIT",
    "author": {
        "name": "Jose Quintana",
        "url": "https://joseluisq.net"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/joseluisq/reactive-http.git"
    },
    "bugs": {
        "url": "https://github.com/joseluisq/reactive-http/issues"
    },
    "keywords": [
        "fetch",
        "http",
        "request",
        "Rx",
        "RxJS",
        "ReactiveX",
        "client",
        "xhr",
        "ES2015"
    ],
    "main": "./src/index.js",
    "types": "./src/index.d.ts",
    "files": [
        "./src/**/*.js",
        "./src/**/*.d.ts",
        "/*.min.js",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "version": "yarn build",
        "clean": "find src examples | grep -E \".d.ts|.js\" | xargs rm -fr",
        "build": "yarn clean && tsc --module commonjs --target es2016",
        "test": "echo \"TODO\"",
        "start": "ts-node examples/index.ts",
        "start:web": "parcel examples/index.html",
        "lint": "yarn clean && eslint --cache \"src/**/*.{js,jsx,ts,tsx}\"",
        "lint:fix": "yarn clean && eslint --cache --fix \"src/**/*.{js,jsx,ts,tsx}\"",
        "docs": "typedoc --out docs src/index.ts",
        "docs:temp": "typedoc --out /tmp/docs src/index.ts"
    },
    "peerDependencies": {
        "rxjs": "^7"
    },
    "devDependencies": {
        "@types/node": "^18.15.3",
        "@typescript-eslint/eslint-plugin": "^5.56.0",
        "@typescript-eslint/parser": "^5.56.0",
        "@typescript/lib-dom": "npm:@types/web",
        "eslint": "^8.36.0",
        "parcel-bundler": "^1.12.5",
        "prettier": "^2.8.6",
        "rxjs": "^7",
        "ts-node": "^10.9.1",
        "typedoc": "^0.23.28",
        "typescript": "^4.9.5"
    }
}
